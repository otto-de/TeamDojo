    },
    "devDependencies": {
        "@angular/cli": "7.3.1",
        "@angular/compiler-cli": "7.2.4",
        "@ngtools/webpack": "7.3.1",
        "@types/jasmine": "3.3.12",
        "@types/jest": "24.0.0",
        "@types/node": "10.12.24",
